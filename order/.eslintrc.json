{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/extensions": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}
