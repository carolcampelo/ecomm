{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "consistent-return": 0,
        "func-names": 0,
        "import/extensions": 0,
        "no-shadow": 0,
        "no-undef": 0,
        "no-underscore-dangle": 0,
        "array-callback-return": 0,
        "no-console": 0,
        "no-plusplus": 0,
        "new-cap": 0,
        "linebreak-style": [
            "error",
            "windows"
        ]
    }
}
